<div class="container-fluid">
  <div class="card shadow mb-4">
    <div class="card-header py-3 d-flex justify-content-between align-items-center">
      <div class="d-flex align-items-center text-mute">
        <i class="fa fa-address-book h3 mr-1"></i>
        <h3 class="font-weight-bold">Contacts</h3>
      </div>   
    </div>        
    <div class="card-body">
      <%= render 'admin/shared/filters' %>
      <div class="table-responsive services-table">
        <%= render 'lists', contacts: @contacts %>
      </div>
    </div>
  </div>
</div>